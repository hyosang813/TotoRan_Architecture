platform :ios, '13.1'

target 'Infra' do
    use_frameworks!
    swift_version = '5.0.0'

    pod "Alamofire",'4.9.0'
    pod "Kanna",'5.0.0'
    pod "RxSwift",'6.1.0'

    target 'TotoRan' do
      use_frameworks!
      swift_version = '5.0.0'
      inherit! :search_paths
    end

    target 'Domain' do
      use_frameworks!
      swift_version = '5.0.0'
      inherit! :search_paths
    end
end
